[core]
        autocrlf = false       # 改行コードを自動変換しない
        ignorecase = false     # 大文字小文字を区別する
        quotepath = false      # パスをクォートせずUTF-8表示
[pull]
        rebase = false         # pull時にrebaseしない
[fetch]
        prune = true           # 取得時に不要ブランチを自動削除
[color]
        ui = auto              # 色表示を自動判定
[include]
        path = config.local    # ローカル設定を読み込む
